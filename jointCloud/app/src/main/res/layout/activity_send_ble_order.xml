<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <include layout="@layout/include_title_layout" />


    <LinearLayout
        android:id="@+id/layout_serarch"
        android:layout_width="match_parent"
        android:layout_height="@dimen/normal_80dp"
        android:background="@color/color_376DE2">

        <EditText
            android:id="@+id/et_value"
            android:layout_width="match_parent"
            android:layout_height="@dimen/normal_50dp"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/normal_30dp"
            android:layout_marginRight="@dimen/normal_30dp"
            android:background="@drawable/shape_white_transparent_bg_20"
            android:drawablePadding="@dimen/normal_12dp"
            android:hint="@string/please_enter_key_words"
            android:paddingLeft="@dimen/normal_17dp"
            android:textColor="@color/color_white"
            android:textColorHint="@color/color_white"
            android:textSize="@dimen/normal_26sp" />
    </LinearLayout>

    <TextView
        android:id="@+id/tv_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/normal_38dp"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/normal_26sp"
        android:text="P03-查询版本号" />


    <TextView
        android:id="@+id/tv_send"
        android:layout_width="match_parent"
        android:layout_height="@dimen/normal_77dp"
        android:layout_marginLeft="@dimen/normal_75dp"
        android:layout_marginTop="@dimen/normal_260dp"
        android:layout_marginRight="@dimen/normal_75dp"
        android:background="@drawable/button_oval_bg"
        android:gravity="center"
        android:text="@string/send_instructions"
        android:textColor="@color/color_white"
        android:textSize="@dimen/normal_34sp" />

    <LinearLayout
        android:id="@+id/type_ly"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/normal_50dp"
        android:paddingLeft="@dimen/normal_40dp"
        android:paddingRight="@dimen/normal_40dp"
        android:visibility="invisible">

        <TextView
            android:layout_width="@dimen/normal_130dp"
            android:layout_height="wrap_content"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/normal_26sp"
            android:text="@string/instruction_type" />

        <TextView
            android:id="@+id/tv_type"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/normal_26sp"
            tools:text="base" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/content_ly"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/normal_20dp"
        android:paddingLeft="@dimen/normal_40dp"
        android:paddingRight="@dimen/normal_40dp"
        android:visibility="invisible">

        <TextView
            android:layout_width="@dimen/normal_130dp"
            android:layout_height="wrap_content"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/normal_26sp"
            android:text="@string/instruction_content" />

        <TextView
            android:id="@+id/tv_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/normal_26sp"
            tools:text="(79100500005,1,001,BASE,1)" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/reback_ly"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/normal_20dp"
        android:paddingLeft="@dimen/normal_40dp"
        android:paddingRight="@dimen/normal_40dp"
        android:visibility="invisible">

        <TextView
            android:layout_width="@dimen/normal_130dp"
            android:layout_height="wrap_content"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/normal_26sp"
            android:text="@string/return_content" />

        <TextView
            android:id="@+id/tv_reback"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/normal_26sp"
            tools:text="(79100500005,1,001,BASE,1)" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/statue_ly"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/normal_20dp"
        android:paddingLeft="@dimen/normal_40dp"
        android:paddingRight="@dimen/normal_40dp"
        android:visibility="invisible">

        <TextView
            android:layout_width="@dimen/normal_130dp"
            android:layout_height="wrap_content"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/normal_26sp"
            android:text="@string/execution_status" />

        <TextView
            android:id="@+id/tv_statue"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/color_0A9C03"
            android:textSize="@dimen/normal_26sp"
            tools:text="base" />
    </LinearLayout>
</LinearLayout>